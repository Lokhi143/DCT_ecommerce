<h2>Listing Sub Categories</h2>
<% if user_signed_in? && current_user.is_admin %>
<%= link_to "Add Sub Category", new_sub_category_path %>
<% end %>

<% if @sub_categories.empty? %>
<p> No category found </p>
<% else %>
 <ol>
   <% @sub_categories.each do |sub_category| %>
	  <li> <%= link_to sub_category.name, sub_category_path(sub_category.id)%> ,
	      <%= sub_category.category_id %>
	    </li>
	<% end %>
  </ol>
	<% end %>
